{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/diflexmo-angular-design",
  "lib": {
    "entryFile": "src/public-api.ts",
    "styleIncludePaths": [
      "./src/lib/styles"
  ]
  },
  "assets": [
    { "input": "./src/assets", "output": "./assets", "glob": "**/*" },
    { "input": "./src/lib/styles", "output": "./lib/styles", "glob": "**/*" }
  ]
}
